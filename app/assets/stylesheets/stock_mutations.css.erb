#listing_sm_products_table_filter label, #listing_sm_products_table_length label {
    font-weight: normal;
}

#listing_sm_products_table tr td:first-child {
    text-align: center;
}

#listing_sm_products_table tr td:first-child:before {
    content: "\f096"; /* fa-square-o */
    font-family: FontAwesome;
}

#listing_sm_products_table tr.selected td:first-child:before {
    content: "\f046"; /* fa-check-square-o */
}

#listing_sm_products_table_wrapper .DTTT_container {
    position: absolute
}