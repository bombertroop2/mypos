#arp_listing_ari_table_filter label, #arp_listing_ari_table_length label {
  font-weight: normal;
}

#arp_listing_ari_table tr td:first-child {
  text-align: center;
}

#arp_listing_ari_table tr td:first-child:before {
  content: "\f096"; /* fa-square-o */
  font-family: FontAwesome;
}

#arp_listing_ari_table tr.selected td:first-child:before {
  content: "\f046"; /* fa-check-square-o */
}

