<td colspan="6">
    <button type="button" class="close cancel" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <div style="padding-top: 20px"></div>

    <dl class="dl-horizontal">
        <dt>Identifier</dt>
        <dd><%= object.identifier %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Name</dt>
        <dd><%= object.name %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Address</dt>
        <dd><%= object.address %></dd>
    </dl>


    <dl class="dl-horizontal">
        <dt>Gender</dt>
        <dd><%= object.gender.capitalize %></dd>
    </dl>


    <dl class="dl-horizontal">
        <dt>Phone</dt>
        <dd><%= remove_empty_space_from_phone_number(object.phone) %></dd>
    </dl>


    <dl class="dl-horizontal">
        <dt>Province</dt>
        <dd><%= object.province %></dd>
    </dl>


    <dl class="dl-horizontal">
        <dt>Warehouse</dt>
        <dd><%= object.warehouse_code %></dd>
    </dl>


    <dl class="dl-horizontal">
        <dt>Mobile phone</dt>
        <dd><%= remove_empty_space_from_phone_number(object.mobile_phone) %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Role</dt>
        <dd><%= object.role %></dd>
    </dl>

</td>