<%= smart_listing_item :account_payables, :create, @account_payable, 
  @account_payable.valid? ? "account_payables/account_payable" : "account_payables/form" %>

<% if @vendor_name.present? %>
  $("#vendor_name_<%= @account_payable.id %>").html("<%= @vendor_name %>");
<% else %>
  $("#return-item-allocation").removeClass("hidden");
<% end %>
