<% if @created %>
  <%= smart_listing_item :account_payable_courier_payments, :create, @account_payable_courier_payment, "account_payable_courier_payments/account_payable_courier_payment" %>
  $("#listing-courier-app-courier-<%= @account_payable_courier_payment.id %>").html("<%= "#{@account_payable_courier_payment.courier.code} - #{@account_payable_courier_payment.courier.name}" %>");
<% else %>
  <% if params[:account_payable_courier_payment][:payment_method].eql?("Cash") %>
    $("#form_ap_payment_container_cash").html("<%= j(render(partial: "form_cash")) %>");
  <% elsif params[:account_payable_courier_payment][:payment_method].eql?("Giro") %>
    $("#form_ap_payment_container_giro").html("<%= j(render(partial: "form_giro")) %>");
  <% else %>
    $("#form_ap_payment_container_transfer").html("<%= j(render(partial: "form_transfer")) %>");
  <% end %>
<% end %>