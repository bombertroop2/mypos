<div class="page-header"><h1>Region Detail</h1></div>
<div class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label">
            Code:
        </label>
        <div class="col-sm-10 form-control hidden-box">
            <%= @region.code %>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">
            Name:
        </label>
        <div class="col-sm-10 form-control hidden-box">
            <%= @region.name %>
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 control-label">
            Description:
        </label>
        <div class="col-sm-10 form-control hidden-box">
            <%= @region.description %>
        </div>
    </div>

</div>
<%= link_to 'Edit', edit_region_path(@region), class: "btn btn-default active" %>
