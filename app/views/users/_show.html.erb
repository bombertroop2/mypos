<td colspan="4">
    <button type="button" class="close cancel" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <div style="padding-top: 20px"></div>

    <dl class="dl-horizontal">
        <dt>Name</dt>
        <dd><%= object.name %></dd>
    </dl>



    <dl class="dl-horizontal">
        <dt>Gender</dt>
        <dd><%= object.gender.capitalize %></dd>
    </dl>




    <dl class="dl-horizontal">
        <dt>Mobile phone</dt>
        <dd><%= remove_empty_space_from_phone_number(object.mobile_phone) %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Email</dt>
        <dd><%= object.email %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Role</dt>
        <dd><%= object.role_name.capitalize %></dd>
    </dl>

</td>