<h1>Listing Warehouses</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Supervisor</th>
      <th>Region</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @warehouses.each do |warehouse| %>
      <tr>
        <td><%= warehouse.code %></td>
        <td><%= warehouse.name %></td>
        <td><%= warehouse.supervisor.name if warehouse.supervisor %></td>
        <td><%= warehouse.region.code if warehouse.region %></td>
        <td><%= warehouse.warehouse_type.titleize %></td>
        <td><%= link_to 'Show', warehouse %></td>
        <td><%= link_to 'Edit', edit_warehouse_path(warehouse) %></td>
        <td><%= link_to 'Destroy', warehouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Warehouse', new_warehouse_path %>
