                                <% tab_id_code = "add_products_by_code_#{counter_event_warehouse.warehouse_id}_cash_discount" %>
                              <% elsif counter_event_type.present? && counter_event_type.eql?("special price") %>
                                <% tab_id_bgm = "add_products_by_bgm_#{counter_event_warehouse.warehouse_id}_special_price" %>
                                <% tab_id_code = "add_products_by_code_#{counter_event_warehouse.warehouse_id}_special_price" %>
                              <% elsif counter_event_type.present? && counter_event_type.eql?("buy one get one") %>
                                <% tab_id_bgm = "add_products_by_bgm_#{counter_event_warehouse.warehouse_id}_buy_one_get_one" %>
                                <% tab_id_code = "add_products_by_code_#{counter_event_warehouse.warehouse_id}_buy_one_get_one" %>
                              <% else %>
                                <% tab_id_bgm = "add_products_by_bgm_#{counter_event_warehouse.warehouse_id}" %>
