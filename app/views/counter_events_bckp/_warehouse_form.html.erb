                                                <div id="listing-warehouse-<%= counter_event_warehouse.warehouse_id %>-products-buy-one-get-one">
                                                  <% else %>
                                                    <div id="listing-warehouse-<%= counter_event_warehouse.warehouse_id %>-products">
                                                      <% end %>
