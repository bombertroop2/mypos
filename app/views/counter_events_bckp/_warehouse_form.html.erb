                                          <% goods_type_field_id = "goods_type_#{counter_event_warehouse.warehouse_id}_special_price" %>
                                        <% elsif counter_event_type.present? && counter_event_type.eql?("buy one get one") %>
                                          <% goods_type_field_id = "goods_type_#{counter_event_warehouse.warehouse_id}_buy_one_get_one" %>
                                        <% else %>
                                          <% goods_type_field_id = "goods_type_#{counter_event_warehouse.warehouse_id}" %>
