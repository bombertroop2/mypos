                                              <% text_field_id = "code_#{counter_event_warehouse.warehouse_id}_cash_discount" %>
                                            <% elsif counter_event_type.present? && counter_event_type.eql?("special price") %>
                                              <% text_field_id = "code_#{counter_event_warehouse.warehouse_id}_special_price" %>
                                            <% elsif counter_event_type.present? && counter_event_type.eql?("buy one get one") %>
                                              <% text_field_id = "code_#{counter_event_warehouse.warehouse_id}_buy_one_get_one" %>
                                            <% else %>
                                              <% text_field_id = "code_#{counter_event_warehouse.warehouse_id}" %>
                                            <% end %>
                                            <div class="form-group">
                                                <label class="control-label col-sm-3" for="code">Code</label>
                                                <div class="col-sm-9">
                                                    <%= text_field_tag "code", "", {class: "form-control", id: text_field_id} %>
                                                  </div>
                                              </div>
                                              <% if counter_event_type.present? && counter_event_type.eql?("cash discount") %>
                                                <% button_code_class = "add-buttons-by-code-cash-discount" %>
                                                <% button_code_id = "add-btn-#{counter_event_warehouse.warehouse_id}-by-code-cash-discount" %>
                                              <% elsif counter_event_type.present? && counter_event_type.eql?("special price") %>
                                                <% button_code_class = "add-buttons-by-code-special-price" %>
                                                <% button_code_id = "add-btn-#{counter_event_warehouse.warehouse_id}-by-code-special-price" %>
                                              <% elsif counter_event_type.present? && counter_event_type.eql?("buy one get one") %>
                                                <% button_code_class = "add-buttons-by-code-buy-one-get-one" %>
                                                <% button_code_id = "add-btn-#{counter_event_warehouse.warehouse_id}-by-code-buy-one-get-one" %>
                                              <% else %>
                                                <% button_code_class = "add-buttons-by-code" %>
                                                <% button_code_id = "add-btn-#{counter_event_warehouse.warehouse_id}-by-code" %>
                                              <% end %>
                                              <div class="form-group">
                                                  <div class="col-sm-offset-3 col-sm-9">
                                                      <button type="submit" class="btn btn-success <%= button_code_class %>" id="<%= button_code_id %>">Add</button>
                                                  </div>
                                              </div>
                                          </div>
                                      </div>
                                      <% if counter_event_type.present? && counter_event_type.eql?("cash discount") %>
                                        <div id="listing-warehouse-<%= counter_event_warehouse.warehouse_id %>-products-cash-discount">
