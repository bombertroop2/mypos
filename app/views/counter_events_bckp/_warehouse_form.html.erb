                                            </div>
                                            <% if counter_event_type.present? && counter_event_type.eql?("cash discount") %>
                                              <% button_bgm_class = "add-buttons-cash-discount" %>
                                              <% button_bgm_id = "add-btn-#{counter_event_warehouse.warehouse_id}-cash-discount" %>
                                            <% elsif counter_event_type.present? && counter_event_type.eql?("special price") %>
                                              <% button_bgm_class = "add-buttons-special-price" %>
                                              <% button_bgm_id = "add-btn-#{counter_event_warehouse.warehouse_id}-special-price" %>
                                            <% elsif counter_event_type.present? && counter_event_type.eql?("buy one get one") %>
                                              <% button_bgm_class = "add-buttons-buy-one-get-one" %>
                                              <% button_bgm_id = "add-btn-#{counter_event_warehouse.warehouse_id}-buy-one-get-one" %>
                                            <% else %>
