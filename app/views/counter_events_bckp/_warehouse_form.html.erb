                                                <div class="col-sm-offset-3 col-sm-9">
                                                    <button type="submit" class="btn btn-success <%= button_bgm_class %>" id="<%= button_bgm_id %>">Add</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div role="tabpanel" class="tab-pane fade" id="<%= tab_id_code %>" style="padding-top: 20px">
