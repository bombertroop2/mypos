                                          <% elsif counter_event_type.present? && counter_event_type.eql?("special price") %>
                                            <% model_field_id = "model_#{counter_event_warehouse.warehouse_id}_special_price" %>
                                          <% elsif counter_event_type.present? && counter_event_type.eql?("buy one get one") %>
                                            <% model_field_id = "model_#{counter_event_warehouse.warehouse_id}_buy_one_get_one" %>
                                          <% else %>
                                            <% model_field_id = "model_#{counter_event_warehouse.warehouse_id}" %>
                                          <% end %>
