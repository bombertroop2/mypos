                              <% select_different_products_field_id = "counter_event_counter_event_warehouses_attributes_#{counter_event_warehouse.warehouse_id}_select_different_products_buy_one_get_one" %>
                              <% select_different_products_field_class_name = "counter_event-warehouse-select-different-products-fields-buy-one-get-one" %>
