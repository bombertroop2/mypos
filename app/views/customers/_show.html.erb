<td colspan="6">
    <button type="button" class="close cancel" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <div style="padding-top: 20px"></div>

    <dl class="dl-horizontal">
        <dt>Code</dt>
        <dd><%= object.code %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Name</dt>
        <dd><%= object.name %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Address</dt>
        <dd><%= object.address %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Phone</dt>
        <dd><%= remove_empty_space_from_phone_number(object.phone) %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Facsimile</dt>
        <dd><%= remove_empty_space_from_phone_number(object.facsimile) %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Email</dt>
        <dd><%= object.email %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>PIC name</dt>
        <dd><%= object.pic_name %></dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>PIC mobile phone</dt>
        <dd><%= remove_empty_space_from_phone_number(object.pic_mobile_phone) %></dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>PIC email</dt>
        <dd><%= object.pic_email %></dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>Terms of payment</dt>
        <dd><%= object.terms_of_payment %> days</dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>Value added tax</dt>
        <dd><%= object.value_added_tax.capitalize %></dd>
    </dl>
    <% unless object.unlimited %>    
      <dl class="dl-horizontal">
          <dt>Limit value</dt>
          <dd><%= number_to_currency(object.limit_value,
                :separator => ",",
                :delimiter => ".",
                :unit => "Rp",
                :precision => 2) %></dd>
      </dl>
    <% end %>
    <dl class="dl-horizontal">
        <dt>Province</dt>
        <dd><%= object.province.name rescue "" %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>City</dt>
        <dd><%= object.city.name rescue "" %></dd>
    </dl>
    <dl class="dl-horizontal">
        <dt>Deliver to</dt>
        <dd><%= object.deliver_to %></dd>
    </dl>

</td>
