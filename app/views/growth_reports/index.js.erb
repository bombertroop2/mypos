<% if params[:date].present? %>
  $("#report-container-daily").html("<%= j(render(partial: "report")) %>");
<% elsif params[:month].present? %>
  $("#report-container").html("<%= j(render(partial: "report")) %>");
<% else %>
  $("#report-container-year").html("<%= j(render(partial: "report")) %>");
<% end %>
