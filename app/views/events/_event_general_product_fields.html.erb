<% if action_name.eql?("edit") %>
  <% event_general_product.prdct_code = event_general_product.product_code %>
  <% event_general_product.prdct_name = event_general_product.product_name %>
<% end %>
<%= fields_for "event[event_general_products_attributes][#{event_general_product.product_id}]", event_general_product do |event_general_product_fields| %>
  <% if event_type.present? && event_type.eql?("cash discount") %>
    <% unless event_general_product_fields.object.new_record? %>
      <%= event_general_product_fields.hidden_field :id, id: "cash_discount_event_general_product_id_#{event_general_product.product_id}" %>
      <%= event_general_product_fields.hidden_field :_destroy, id: "cash_discount_event_general_product_destroy_#{event_general_product.product_id}" %>
    <% end %>
    <% general_product_fields_class_name = "" %>
    <% unless @event.new_record? %>
      <% general_product_fields_class_name = " event-general-products-fields-cash-discount" %>
    <% end %>
    <%= event_general_product_fields.hidden_field :product_id, class: "event-general-products-#{event_general_product.product_id}-cash-discount#{general_product_fields_class_name} event-general-products-product-id-fields-cash-discount" %>
      <%= event_general_product_fields.hidden_field :prdct_code, class: "event-general-products-#{event_general_product.product_id}-cash-discount#{general_product_fields_class_name}" %>
        <%= event_general_product_fields.hidden_field :prdct_name, class: "event-general-products-#{event_general_product.product_id}-cash-discount#{general_product_fields_class_name}" %>
        <% elsif event_type.present? && event_type.eql?("special price") %>
          <% unless event_general_product_fields.object.new_record? %>
            <%= event_general_product_fields.hidden_field :id, id: "special_price_event_general_product_id_#{event_general_product.product_id}" %>
            <%= event_general_product_fields.hidden_field :_destroy, id: "special_price_event_general_product_destroy_#{event_general_product.product_id}" %>
          <% end %>
          <% general_product_fields_class_name = "" %>
          <% unless @event.new_record? %>
            <% general_product_fields_class_name = " event-general-products-fields-special-price" %>
          <% end %>
          <%= event_general_product_fields.hidden_field :product_id, class: "event-general-products-#{event_general_product.product_id}-special-price#{general_product_fields_class_name} event-general-products-product-id-fields-special-price" %>
            <%= event_general_product_fields.hidden_field :prdct_code, class: "event-general-products-#{event_general_product.product_id}-special-price#{general_product_fields_class_name}" %>
              <%= event_general_product_fields.hidden_field :prdct_name, class: "event-general-products-#{event_general_product.product_id}-special-price#{general_product_fields_class_name}" %>
              <% elsif event_type.present? && event_type.eql?("buy one get one") %>
                <% unless event_general_product_fields.object.new_record? %>
                  <%= event_general_product_fields.hidden_field :id, id: "buy_one_get_one_event_general_product_id_#{event_general_product.product_id}" %>
                  <%= event_general_product_fields.hidden_field :_destroy, id: "buy_one_get_one_event_general_product_destroy_#{event_general_product.product_id}" %>
                <% end %>
                <% general_product_fields_class_name = "" %>
                <% unless @event.new_record? %>
                  <% general_product_fields_class_name = " event-general-products-fields-buy-one-get-one" %>
                <% end %>
                <%= event_general_product_fields.hidden_field :product_id, class: "event-general-products-#{event_general_product.product_id}-buy-one-get-one#{general_product_fields_class_name} event-general-products-product-id-fields-buy-one-get-one" %>
                  <%= event_general_product_fields.hidden_field :prdct_code, class: "event-general-products-#{event_general_product.product_id}-buy-one-get-one#{general_product_fields_class_name}" %>
                    <%= event_general_product_fields.hidden_field :prdct_name, class: "event-general-products-#{event_general_product.product_id}-buy-one-get-one#{general_product_fields_class_name}" %>
                    <% elsif event_type.present? && event_type.eql?("gift") %>
                      <% unless event_general_product_fields.object.new_record? %>
                        <%= event_general_product_fields.hidden_field :id, id: "gift_event_general_product_id_#{event_general_product.product_id}" %>
                        <%= event_general_product_fields.hidden_field :_destroy, id: "gift_event_general_product_destroy_#{event_general_product.product_id}", class: "event-general-products-destroy-fields-gift" %>
                        <% end %>
                        <% general_product_fields_class_name = "" %>
                        <% unless @event.new_record? %>
                          <% general_product_fields_class_name = " event-general-products-fields-gift" %>
                        <% end %>
                        <%= event_general_product_fields.hidden_field :product_id, class: "event-general-products-#{event_general_product.product_id}-gift#{general_product_fields_class_name} event-general-products-product-id-fields-gift" %>
                          <%= event_general_product_fields.hidden_field :prdct_code, class: "event-general-products-#{event_general_product.product_id}-gift#{general_product_fields_class_name}" %>
                            <%= event_general_product_fields.hidden_field :prdct_name, class: "event-general-products-#{event_general_product.product_id}-gift#{general_product_fields_class_name}" %>
                            <% else %>
                              <% unless event_general_product_fields.object.new_record? %>
                                <%= event_general_product_fields.hidden_field :id %>
                                <%= event_general_product_fields.hidden_field :_destroy %>
                              <% end %>
                              <% general_product_fields_class_name = "" %>
                              <% unless @event.new_record? %>
                                <% general_product_fields_class_name = " event-general-products-fields-percentage-discount" %>
                              <% end %>
                              <%= event_general_product_fields.hidden_field :product_id, class: "event-general-products-#{event_general_product.product_id}-percentage-discount#{general_product_fields_class_name} event-general-products-product-id-fields-percentage-discount" %>
                                <%= event_general_product_fields.hidden_field :prdct_code, class: "event-general-products-#{event_general_product.product_id}-percentage-discount#{general_product_fields_class_name}" %>
                                  <%= event_general_product_fields.hidden_field :prdct_name, class: "event-general-products-#{event_general_product.product_id}-percentage-discount#{general_product_fields_class_name}" %>
                                  <% end %>
                                <% end %>
