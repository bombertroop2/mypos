<% if params[:event_type].present? && params[:event_type].eql?("cash discount") %>
  $("#listing-warehouse-details-cash").html("<%= j(render(partial: "warehouse_form")) %>");
<% elsif params[:event_type].present? && params[:event_type].eql?("special price") %>
  $("#listing-warehouse-details-sp").html("<%= j(render(partial: "warehouse_form")) %>");
<% elsif params[:event_type].present? && params[:event_type].eql?("buy one get one") %>
  $("#listing-warehouse-details-bogo").html("<%= j(render(partial: "warehouse_form")) %>");
<% elsif params[:event_type].present? && params[:event_type].eql?("gift") %>
  $("#listing-warehouse-details-gift").html("<%= j(render(partial: "warehouse_form_gift")) %>");
<% else %>
  $("#listing-warehouse-details").html("<%= j(render(partial: "warehouse_form")) %>");
<% end %>
