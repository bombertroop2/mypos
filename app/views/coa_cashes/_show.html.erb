<td colspan="4">
    <button type="button" class="close cancel" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <div style="padding-top: 20px"></div>

    <dl class="dl-horizontal">
        <dt>Date</dt>
        <dd><%= object.date.strftime("%d/%m/%Y")%></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Coa</dt>
        <dd><%= object.coa.code %>-<%= object.coa.transaction_type %></dd>
    </dl>

    <dl class="dl-horizontal">
        <dt>Value</dt>
        <dd><%= number_to_currency(object.value, :separator => ",", :delimiter => ".", :unit => "Rp", :precision => 2) rescue "" %></dd>
    </dl>

</td>
