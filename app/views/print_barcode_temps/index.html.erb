<p id="notice"><%= notice %></p>

<h1>Print Barcode Temps</h1>

<table>
  <thead>
    <tr>
      <th>Product code</th>
      <th>Brand name</th>
      <th>Product description</th>
      <th>Barcode</th>
      <th>Size</th>
      <th>Color name</th>
      <th>Price</th>
      <th>Internet client address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @print_barcode_temps.each do |print_barcode_temp| %>
      <tr>
        <td><%= print_barcode_temp.product_code %></td>
        <td><%= print_barcode_temp.brand_name %></td>
        <td><%= print_barcode_temp.product_description %></td>
        <td><%= print_barcode_temp.barcode %></td>
        <td><%= print_barcode_temp.size %></td>
        <td><%= print_barcode_temp.color_name %></td>
        <td><%= print_barcode_temp.price %></td>
        <td><%= print_barcode_temp.internet_client_address %></td>
        <td><%= link_to 'Show', print_barcode_temp %></td>
        <td><%= link_to 'Edit', edit_print_barcode_temp_path(print_barcode_temp) %></td>
        <td><%= link_to 'Destroy', print_barcode_temp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Print Barcode Temp', new_print_barcode_temp_path %>
