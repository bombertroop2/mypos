<td><%= object.code %></td>
<td><%= object.name %></td>
<td><%= object.company.code %></td>
<td><%= object.transaction_type_detail %></td>
<td><%= object.description %></td>
<td class="actions" style="text-align: right">
    <%= link_to "<i class=\"glyphicon glyphicon-share-alt\"></i>".html_safe, object, data: {remote: true}, title: "Show" %>
    <%= link_to_if(can?(:manage, Coa), content_tag(:i, "", :class => "glyphicon glyphicon-pencil"), edit_coa_path(object), :remote => true, class: :edit, method: :get, title: "Edit" ) %>
      <%= link_to_if(can?(:manage, Coa), content_tag(:i, "", :class => "glyphicon glyphicon-trash"), object, :remote => true, :class => "destroy", :method => :delete, :data => {:confirmation => 'Are you sure that you want to permanently delete the selected item?', placement: :left}, title: "Delete" ) %>
  </td>
