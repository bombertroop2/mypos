<table class="table table-striped table-bordered" style="table-layout: fixed;word-wrap: break-word;">
    <thead id="listing-stock-transactions-thead">
        <tr>
            <th class="col-md-4 text-center" colspan="3">Transaction</th>
            <th class="col-md-5 text-center" colspan="4">Article</th>
            <th class="col-md-1 text-center" rowspan="2" style="vertical-align: middle"><%= smart_listing.sortable "Qty", "quantity" %></th>
            <th class="col-md-2 text-center" rowspan="2" style="vertical-align: middle"><%= smart_listing.sortable "Total Price", "total_product_price" %></th>
        </tr>
        <tr>
            <th class="text-center"><%= smart_listing.sortable "Date", "transaction_date" %></th>
            <th class="text-center"><%= smart_listing.sortable "Number", "transaction_number" %></th>
            <th class="text-center"><%= smart_listing.sortable "Type", "transaction_type" %></th>
            <th class="text-center"><%= smart_listing.sortable "Article", "products.code" %></th>
            <th class="text-center"><%= smart_listing.sortable "Color", "common_fields.code" %></th>
            <th class="text-center"><%= smart_listing.sortable "Size", "size" %></th>
            <th class="text-center"><%= smart_listing.sortable "Price", "product_first_price" %></th>
        </tr>
    </thead>
    <tbody>
        <% total_qty = 0 %>
        <% total_price = 0 %>
        <% last_transaction_date = nil %>
        <% last_transaction_number = "" %>
        <% last_transaction_type = "" %>
        <% last_product_code = "" %>
        <% last_color_code = "" %>
        <% last_size = "" %>
        <% last_product_price = "" %>
        <% collection = smart_listing.collection %>
        <% collection.each do |stock_transaction| %>
          <% if stock_transaction.transaction_type.eql?("BS") %>
            <% total_qty += stock_transaction.quantity %>
            <% total_price += stock_transaction.total_product_price %>
          <% elsif stock_transaction.transaction_type.eql?("PO") %>
            <% total_qty += stock_transaction.quantity %>
            <% total_price += stock_transaction.total_product_price %>
          <% elsif stock_transaction.transaction_type.eql?("PR") %>
            <% total_qty -= stock_transaction.quantity %>
            <% total_price -= stock_transaction.total_product_price %>
          <% elsif stock_transaction.transaction_type.eql?("DO") %>
            <% if stock_transaction.warehouse_type.eql?("central") %>
              <% total_qty -= stock_transaction.quantity %>
              <% total_price -= stock_transaction.total_product_price %>
            <% else %>
              <% total_qty += stock_transaction.quantity %>
              <% total_price += stock_transaction.total_product_price %>
            <% end %>
          <% elsif stock_transaction.transaction_type.eql?("RW") %>
            <% if stock_transaction.warehouse_type.eql?("central") %>
              <% total_qty += stock_transaction.quantity %>
              <% total_price += stock_transaction.total_product_price %>
            <% else %>
              <% total_qty -= stock_transaction.quantity %>
              <% total_price -= stock_transaction.total_product_price %>
            <% end %>
          <% elsif stock_transaction.transaction_type.eql?("RGO") %>
            <% total_qty -= stock_transaction.quantity %>
            <% total_price -= stock_transaction.total_product_price %>
          <% elsif stock_transaction.transaction_type.eql?("RGI") %>
            <% total_qty += stock_transaction.quantity %>
            <% total_price += stock_transaction.total_product_price %>
          <% elsif stock_transaction.transaction_type.eql?("POS") %>
            <% total_qty -= stock_transaction.quantity %>
            <% total_price -= stock_transaction.total_product_price %>
          <% elsif stock_transaction.transaction_type.eql?("RET") %>
            <% total_qty += stock_transaction.quantity %>
            <% total_price += stock_transaction.total_product_price %>
          <% elsif stock_transaction.transaction_type.eql?("SLK") %>
            <% total_qty -= stock_transaction.quantity %>
            <% total_price -= stock_transaction.total_product_price %>
          <% end %>
          <tr class="editable" data-id="<%#= brand.id %>">
              <% if params[:listing_stock_transactions_smart_listing][:sort].blank? || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_date") %>
                <% total_transaction = collection.select{|st| st.transaction_date == stock_transaction.transaction_date}.length %>
                <% total_transaction_number = collection.select{|st| st.transaction_date == stock_transaction.transaction_date && st.transaction_number.eql?(stock_transaction.transaction_number)}.length %>
                <% total_transaction_type = collection.select{|st| st.transaction_date == stock_transaction.transaction_date && st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type)}.length %>
                <% total_product_code = collection.select{|st| st.transaction_date == stock_transaction.transaction_date && st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code)}.length %>
                <% total_color_code = collection.select{|st| st.transaction_date == stock_transaction.transaction_date && st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code) && st.color_code.eql?(stock_transaction.color_code)}.length %>
                <% total_size = collection.select{|st| st.transaction_date == stock_transaction.transaction_date && st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code) && st.color_code.eql?(stock_transaction.color_code) && st.size.eql?(stock_transaction.size)}.length %>
                <% total_product_price = collection.select{|st| st.transaction_date == stock_transaction.transaction_date && st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code) && st.color_code.eql?(stock_transaction.color_code) && st.size.eql?(stock_transaction.size) && st.product_first_price == stock_transaction.product_first_price}.length %>
                <% unless last_transaction_date == stock_transaction.transaction_date %>
                  <td rowspan="<%= total_transaction %>"><%= stock_transaction.transaction_date.strftime("%d/%m/%Y") %></td>
                <% end %>
              <% else %>
                <td><%= stock_transaction.transaction_date.strftime("%d/%m/%Y") %></td>
              <% end %>

              <% if params[:listing_stock_transactions_smart_listing][:sort].blank? || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_date") || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_number") %>
                <% if params[:listing_stock_transactions_smart_listing][:sort].present? && params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_number") %>
                  <% total_transaction_number = collection.select{|st| st.transaction_number.eql?(stock_transaction.transaction_number)}.length %>
                  <% total_transaction_type = collection.select{|st| st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type)}.length %>
                  <% total_product_code = collection.select{|st| st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code)}.length %>
                  <% total_color_code = collection.select{|st| st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code) && st.color_code.eql?(stock_transaction.color_code)}.length %>
                  <% total_size = collection.select{|st| st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code) && st.color_code.eql?(stock_transaction.color_code) && st.size.eql?(stock_transaction.size)}.length %>
                  <% total_product_price = collection.select{|st| st.transaction_number.eql?(stock_transaction.transaction_number) && st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code) && st.color_code.eql?(stock_transaction.color_code) && st.size.eql?(stock_transaction.size) && st.product_first_price == stock_transaction.product_first_price}.length %>
                <% end %>
                <% if (params[:listing_stock_transactions_smart_listing][:sort].blank? || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_date")) &&
                    (last_transaction_date != stock_transaction.transaction_date ||
                      (last_transaction_date == stock_transaction.transaction_date && !last_transaction_number.eql?(stock_transaction.transaction_number))) %>
                  <td rowspan="<%= total_transaction_number %>"><%= stock_transaction.transaction_number %></td>
                <% elsif params[:listing_stock_transactions_smart_listing][:sort].present? && params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_number") && !last_transaction_number.eql?(stock_transaction.transaction_number) %>
                  <td rowspan="<%= total_transaction_number %>"><%= stock_transaction.transaction_number %></td>
                <% end %>
              <% else %>
                <td><%= stock_transaction.transaction_number %></td>
              <% end %>

              <% if params[:listing_stock_transactions_smart_listing][:sort].blank? || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_date") || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_number") || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_type") %>
                <% if params[:listing_stock_transactions_smart_listing][:sort].present? && params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_type") %>
