              <% end %>

              <% if params[:listing_stock_transactions_smart_listing][:sort].blank? || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_date") || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_number") || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_type") || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("products.code") || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("common_fields.code") %>
