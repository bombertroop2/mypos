                    (!last_transaction_number.eql?(stock_transaction.transaction_number) ||
                      (last_transaction_number.eql?(stock_transaction.transaction_number) && !last_transaction_type.eql?(stock_transaction.transaction_type)) ||
                      (last_transaction_number.eql?(stock_transaction.transaction_number) && last_transaction_type.eql?(stock_transaction.transaction_type) && !last_product_code.eql?(stock_transaction.product_code))) %>
                  <td rowspan="<%= total_product_code %>"><%= "#{stock_transaction.product_code} - #{stock_transaction.product_name}" %></td>
                <% elsif params[:listing_stock_transactions_smart_listing][:sort].present? && params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_type") &&
                    (!last_transaction_type.eql?(stock_transaction.transaction_type) ||
                      (last_transaction_type.eql?(stock_transaction.transaction_type) && !last_product_code.eql?(stock_transaction.product_code))) %>
                  <td rowspan="<%= total_product_code %>"><%= "#{stock_transaction.product_code} - #{stock_transaction.product_name}" %></td>
                <% elsif params[:listing_stock_transactions_smart_listing][:sort].present? && params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("products.code") &&
