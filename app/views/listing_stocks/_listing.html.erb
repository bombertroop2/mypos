                  <% total_product_price = collection.select{|st| st.transaction_type.eql?(stock_transaction.transaction_type) && st.product_code.eql?(stock_transaction.product_code) && st.color_code.eql?(stock_transaction.color_code) && st.size.eql?(stock_transaction.size) && st.product_first_price == stock_transaction.product_first_price}.length %>
                <% end %>
                <% if (params[:listing_stock_transactions_smart_listing][:sort].blank? || params[:listing_stock_transactions_smart_listing][:sort].keys.first.eql?("transaction_date")) &&
                    (last_transaction_date != stock_transaction.transaction_date ||
                      (last_transaction_date == stock_transaction.transaction_date && !last_transaction_number.eql?(stock_transaction.transaction_number)) ||
