<% if @vendor_name.present? %>
  <%= smart_listing_item :purchase_returns, :create, @purchase_return, "purchase_returns/purchase_return" %>
  $("#pr-number-<%= @purchase_return.id %>").html("<%= @pr_number %>");
  $("#pr-vendor-name-<%= @purchase_return.id %>").html("<%= @vendor_name %>");
<% else %>
  <%#= smart_listing_item :purchase_returns, :create, @purchase_return, "purchase_returns/form" %>
  $("#pr_form_container_dp").html("<%= j(render(partial: "pr_form_dp")) %>");
<% end %>