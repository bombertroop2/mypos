<td><%= object.warehouse.code %></td>
<td><%= object.month_name %></td>
<td><%= object.year %></td>
<td><%= number_to_currency(object.target_value, :separator => ",", :delimiter => ".", :unit => "Rp", :precision => 2) %></td>
<td class="actions" style="text-align: right">
    <%= link_to "<i class=\"glyphicon glyphicon-share-alt\"></i>".html_safe, object, data: {remote: true}, title: "Show" %>
    <%= link_to_if(can?(:manage, Target), content_tag(:i, "", :class => "glyphicon glyphicon-pencil"), edit_target_path(object), :remote => true, class: :edit, method: :get, title: "Edit" ) %>
      <%= link_to_if(can?(:manage, Target), content_tag(:i, "", :class => "glyphicon glyphicon-trash"), object, :remote => true, :class => "destroy", :method => :delete, :data => {:confirmation => 'Are you sure that you want to permanently delete the selected item?', placement: :left}, title: "Delete" ) %>
  </td>
